cmd "01_core" {
    exec = "roscore";
    host = "localhost";
}

cmd "02_start_seadrone" {
    exec = "roslaunch joystick_control seadrone_start.launch rviz:=false camera:=false veh:=seadrone";
    host = "localhost";
}

cmd "03a_camera" {
    exec = "rosrun seadrone_sensor cam.py";
    host = "localhost";
}

cmd "rviz" {
    exec = "roslaunch seadrone_sensor rviz.launch";
    host = "localhost";
}



script "start_seadrone" 
{
    start cmd "01_core" wait "running";
    wait ms 1500;
    start cmd "02_start_seadrone" wait "running";
    wait ms 1500;
}
